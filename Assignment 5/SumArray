#include <iostream>
#define N 3

int sumArray(int A[N][N][N]) {
    int i, j, k, sum = 0;

    for (i = 0; i < N; i++)
        for (j = 0; j < N; j++)
            for (k = 0; k < N; k++) {
                std::cout << "Adding A[" << k << "][" << i << "][" << j << "] = " << A[k][i][j] << " to sum\n";
                sum += A[k][i][j];
            }
    return sum;
}